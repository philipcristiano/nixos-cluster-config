{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackageNames": ["minio/minio"],
      "versionCompatibility": "^(?<compatibility>.*).(?<version>.*)T.*$",
      "versioning": "loose"
    }
  ]
}
